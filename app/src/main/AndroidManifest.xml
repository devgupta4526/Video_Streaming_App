<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.underground.VideoGram">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.underground.VideoGram.commentpanel"></activity>
        <activity android:name="com.underground.VideoGram.update_profile" />
        <activity android:name="com.underground.VideoGram.addvideo" />
        <activity android:name="com.underground.VideoGram.dashboard" />
        <activity android:name="com.underground.VideoGram.login" />
        <activity android:name="com.underground.VideoGram.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>